import java.util.Scanner;
import java.lang.String;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String name = sc.next();
        switch (name) {
            case "Вячеслав":
                System.out.println("Привет, Вячеслав");
                break;
            default :
                System.out.println("Нет такого имени");
        }
    }
}
